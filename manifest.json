{
    "name": "YouTube Video Rating",
    "description": "Gauge the quality of the YouTube video with basic metrics available openly",
    "version": "0.3",
    "manifest_version": 3,
    "background": {
        "service_worker": "get_metrics.js"
    },
    "action": {
        "default_interface": "rating_interface.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    }
}